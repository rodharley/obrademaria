<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://ecommerce.cbmp.com.br" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://ecommerce.cbmp.com.br">

    <xsd:complexType name="RequisicaoNovaTransacao">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>
    				<xsd:element name="dados-ec"
    					type="tns:DadosEcAutenticacao" maxOccurs="1" minOccurs="0">
    				</xsd:element>
    				<xsd:element name="dados-portador"
    					type="tns:DadosCartao" maxOccurs="1" minOccurs="0">
    				</xsd:element>
    				<xsd:element name="dados-pedido"
    					type="tns:DadosPedido" maxOccurs="1" minOccurs="1">
    				</xsd:element>
                                
    				<xsd:element name="forma-pagamento"
    					type="tns:FormaPagamento" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="url-retorno" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:minLength value="0"></xsd:minLength>
    							<xsd:maxLength value="1024"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="autorizar" maxOccurs="1"
    					minOccurs="1">
    					<xsd:annotation>
    						<xsd:documentation>
    							0 - Nao autorizar 1 - Autorizar somente
    							se autenticada 2 - Autorizar
    							nao-autenticada e autenticada 3 -
    							Somente autorizar (nao realizar
    							autenticacao) 4 - autorizacao recorrente
    						</xsd:documentation>
    					</xsd:annotation>
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:enumeration value="0"></xsd:enumeration>
    							<xsd:enumeration value="1"></xsd:enumeration>
    							<xsd:enumeration value="2"></xsd:enumeration>
    							<xsd:enumeration value="3"></xsd:enumeration>
    							<xsd:enumeration value="4"></xsd:enumeration>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="capturar" type="xsd:boolean"
    					maxOccurs="1" minOccurs="1">
    					<xsd:annotation>
    						<xsd:documentation>
    							Define se a transacao sera capturada
    							apos uma autorizacao com sucesso.
    						</xsd:documentation>
    					</xsd:annotation>
    				</xsd:element>
    				<xsd:element name="campo-livre" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:maxLength value="128"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="bin" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:pattern value="[0-9]{6}"></xsd:pattern>
    							<xsd:length value="6"></xsd:length>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="gerar-token" type="xsd:boolean"
    					maxOccurs="1" minOccurs="0">
    					<xsd:annotation>
    						<xsd:documentation>
    							Define se a transacao deverá gerar um
    							token para o "Cartão + EC".
    						</xsd:documentation>
    					</xsd:annotation>
    				</xsd:element>

    				<xsd:element name="avs" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
                                
                    <xsd:element name="dados-autenticacao-mpi-externa"
    					type="tns:DadosAutenticacaoMPIExterna" maxOccurs="1" minOccurs="0">
                    </xsd:element>
					
					<xsd:element name="analise-fraude"
    					type="tns:AnaliseFraude" maxOccurs="1" minOccurs="0">
    				</xsd:element>					
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="RequisicaoCaptura">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Requisicao">
    			<xsd:sequence>
    				<xsd:element name="valor" maxOccurs="1" minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:pattern value="[0-9]{1,9}"></xsd:pattern>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="anexo" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    				<xsd:element name="taxa-embarque" type="tns:taxaEmbarqueType" maxOccurs="1" minOccurs="0"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>


    <xsd:complexType name="RequisicaoCancelamento">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Requisicao">
    			<xsd:sequence>
    				<xsd:element name="valor" maxOccurs="1" minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    						<xsd:pattern value="[0-9]{1,9}"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="RequisicaoConsulta">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Requisicao"></xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    

    <xsd:complexType name="RequisicaoAutorizacaoTid">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Requisicao"></xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="DadosPedido">
    	<xsd:sequence>
    		<xsd:element name="numero" maxOccurs="1" minOccurs="1"
    			type="tns:numeroPedidoType">
    		</xsd:element>
    		
    		<xsd:element name="valor" minOccurs="1" maxOccurs="1">
    			<xsd:annotation>
    				<xsd:documentation>
    					Valor com formato somente numeros Ex: 1.020,99
    					deve ser enviado como 102099
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:int">
    					<xsd:minInclusive value="1"></xsd:minInclusive>
    					<xsd:maxInclusive value="999999999"></xsd:maxInclusive> <!-- 1-9  -->
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		
    		
    		<xsd:element name="moeda" minOccurs="1" maxOccurs="1">
    			<xsd:annotation>
    				<xsd:documentation>
    					ISO http://en.wikipedia.org/wiki/ISO_4217.Para
    					moeda brasileira, 986
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:int">
    					<xsd:minInclusive value="0"></xsd:minInclusive>
    					<xsd:maxInclusive value="999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="data-hora" type="xsd:dateTime"
    			minOccurs="1" maxOccurs="1">
    			<xsd:annotation>
    				<xsd:documentation>
    					Formato Brasileiro ex: 15/01/2009 17:00:00
    				</xsd:documentation>
    			</xsd:annotation>
    		</xsd:element>
    		<xsd:element name="descricao" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:maxLength value="1024"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="idioma" maxOccurs="1" minOccurs="0"
    			type="tns:idiomaType">
    		</xsd:element>
    		<xsd:element name="taxa-embarque" type="tns:taxaEmbarqueType" maxOccurs="1" minOccurs="0"></xsd:element>
    		<xsd:element name="soft-descriptor" type="tns:softDescriptorType" minOccurs="0" maxOccurs="1"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="RequisicaoAutorizacaoPortador">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Requisicao">
    			<xsd:sequence>
    				<xsd:element name="dados-cartao"
    					type="tns:DadosCartao" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="dados-pedido"
    					type="tns:DadosPedido" maxOccurs="1" minOccurs="1">
    				</xsd:element>

    				<xsd:element name="forma-pagamento"
    					type="tns:FormaPagamento" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="capturar-automaticamente"
    					type="xsd:boolean" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="campo-livre" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:maxLength value="128"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="avs" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="Requisicao">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>
    				<xsd:element name="tid" type="tns:tidType">
    				</xsd:element>
    				<xsd:element name="dados-ec" type="tns:DadosEc"></xsd:element>
    			</xsd:sequence>

    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

	<xsd:complexType name="RequisicaoConsultaChSec">
		<xsd:complexContent>
			<xsd:extension base="tns:Mensagem">
				<xsd:sequence>
					<xsd:element name="numero-pedido" type="tns:numeroPedidoType">
					</xsd:element>
					<xsd:element name="dados-ec" type="tns:DadosEc"></xsd:element>
				</xsd:sequence>

			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="RequisicaoToken">
		<xsd:complexContent>
			<xsd:extension base="tns:Mensagem">
				<xsd:sequence>					
					<xsd:element name="dados-ec" type="tns:DadosEc"></xsd:element>
					<xsd:element name="dados-portador" type="tns:DadosCartao"></xsd:element>
				</xsd:sequence>

			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
    <xsd:element name="requisicao-autenticacao" type="tns:RequisicaoNovaTransacao"></xsd:element>

    <xsd:complexType name="DadosEc">
    	<xsd:sequence>
    		<xsd:element name="numero" minOccurs="1" maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[0-9]{1,20}"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="chave" maxOccurs="1" minOccurs="1">
                <xsd:annotation>
                	<xsd:documentation>
                		Campo informado pela Cielo no momento do
                		cadastro do EC
                	</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:minLength value="1"></xsd:minLength>
    					<xsd:maxLength value="100"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Mensagem">
    	<xsd:sequence>
    	</xsd:sequence>
    	<xsd:attribute name="id" use="required" type="xsd:string">
    	</xsd:attribute>
    	<xsd:attribute name="versao" use="required">
    		<xsd:simpleType>
    			<xsd:restriction base="xsd:string">
    				<xsd:pattern value="[0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,2}"></xsd:pattern>
    				<xsd:enumeration value="1.0.0"></xsd:enumeration>
    				<xsd:enumeration value="1.1.0"></xsd:enumeration>
					<xsd:enumeration value="1.1.1"></xsd:enumeration>    				
					<xsd:enumeration value="1.2.0"></xsd:enumeration>
					<xsd:enumeration value="1.2.1"></xsd:enumeration>
                    <xsd:enumeration value="1.3.0"></xsd:enumeration>
					<xsd:enumeration value="1.4.0"></xsd:enumeration>
    			</xsd:restriction>
    		</xsd:simpleType>
    	</xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="DadosCartao">
    	<xsd:sequence>
    		<xsd:element name="numero" minOccurs="0" maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[0-9]{0,19}"></xsd:pattern>    					
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="validade" maxOccurs="1" minOccurs="0">
    			<xsd:annotation>
    				<xsd:documentation>
    					Formato YYYYMM
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[0-9]{0,6}"></xsd:pattern>   					
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="indicador" maxOccurs="1" minOccurs="0">
    			<xsd:annotation>
    				<xsd:documentation>
    					0 - Nao informado 1 - Informado 2 - Ilegivel 9 -
    					Inexistente
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:enumeration value="0"></xsd:enumeration>
    					<xsd:enumeration value="1"></xsd:enumeration>
    					<xsd:enumeration value="2"></xsd:enumeration>
    					<xsd:enumeration value="9"></xsd:enumeration>
    					<xsd:minLength value="0"></xsd:minLength>
    					<xsd:maxLength value="1"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="codigo-seguranca" maxOccurs="1" minOccurs="0">
    			<xsd:annotation>
    				<xsd:documentation></xsd:documentation>
    			</xsd:annotation>

    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="\d{0}|\d{3,4}"></xsd:pattern>    					
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="nome-portador" maxOccurs="1"
    			minOccurs="0">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:minLength value="1"></xsd:minLength>
    					<xsd:maxLength value="50"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="token" type="tns:tokenType" maxOccurs="1" minOccurs="0"></xsd:element>
		    <xsd:element name="cnpj-cpf-portador" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[0-9]{14}|[0-9]{11}"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="tipo-pessoa" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:minLength value="1"></xsd:minLength>
    					<xsd:maxLength value="1"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:element name="requisicao-autorizacao-portador" type="tns:RequisicaoAutorizacaoPortador"></xsd:element>

    <xsd:element name="requisicao-autorizacao-tid" type="tns:RequisicaoAutorizacaoTid"></xsd:element>

    <xsd:element name="requisicao-cancelamento" type="tns:RequisicaoCancelamento"></xsd:element>

    <xsd:element name="requisicao-captura" type="tns:RequisicaoCaptura"></xsd:element>

    <xsd:element name="requisicao-consulta" type="tns:RequisicaoConsulta"></xsd:element>

	<xsd:element name="requisicao-consulta-chsec" type="tns:RequisicaoConsultaChSec"></xsd:element>
	
	<xsd:element name="requisicao-token" type="tns:RequisicaoToken"></xsd:element>
	

    <xsd:complexType name="FormaPagamento">
		<xsd:sequence>
			<xsd:element name="bandeira" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<!-- <xsd:pattern value="\w{1,15}"></xsd:pattern> -->
						<xsd:enumeration value="visa"></xsd:enumeration>
						<xsd:enumeration value="mastercard"></xsd:enumeration>
						<xsd:enumeration value="elo"></xsd:enumeration>
						<xsd:enumeration value="diners"></xsd:enumeration>
						<xsd:enumeration value="discover"></xsd:enumeration>
						<xsd:enumeration value="amex"></xsd:enumeration>
						<xsd:enumeration value="aura"></xsd:enumeration>
						<xsd:enumeration value="jcb"></xsd:enumeration>
						<xsd:enumeration value="informação não disponível no momento"></xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
            <xsd:element name="produto" maxOccurs="1" minOccurs="1">
    			<xsd:annotation>
    				<xsd:documentation>
    					1 - Crédito a Vista 2 - Parcelado Loja 3 - Parcelado Adm A - Visa Electron
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="(1|2|3|A)"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="parcelas" maxOccurs="1" minOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:int">
    					<xsd:minInclusive value="1"></xsd:minInclusive>
    					<xsd:maxInclusive value="999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="DadosToken">
    	<xsd:sequence>    		
			<xsd:element name="codigo-token" maxOccurs="1" minOccurs="1" type="tns:tokenType"></xsd:element>
				
    		<xsd:element name="status" maxOccurs="1" minOccurs="1">
    					<xsd:annotation>
    						<xsd:documentation>
    							0 - Bloqueado 1 - Desbloqueado
    						</xsd:documentation>
    					</xsd:annotation>
    					<xsd:simpleType >
    						<xsd:restriction base="xsd:string">
    							<xsd:pattern value="(0|1)"></xsd:pattern>
    						</xsd:restriction>
    					</xsd:simpleType>
    		</xsd:element>	
    		
    		

			<xsd:element name="numero-cartao-truncado" minOccurs="1" maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[a-zA-Z0-9]{1,19}"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>    		
    		
    	</xsd:sequence>
    </xsd:complexType>
    

    <xsd:complexType name="DadosEcAutenticacao">
    	<xsd:complexContent>
    		<xsd:extension base="tns:DadosEc">
    			<xsd:sequence>
                    <xsd:element name="nome" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:maxLength value="25"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="codigo-pais" maxOccurs="1"
                    	minOccurs="0">
                    	<xsd:annotation>
                    		<xsd:documentation>
                    			ISO 3166 (Numerica) http://en.wikipedia.org/wiki/ISO_3166-1_numeric
                    			Para o Brasil o codigo e 076.
                    		</xsd:documentation>
                    	</xsd:annotation>
                    	<xsd:simpleType>
                    		<xsd:restriction base="xsd:string">
                    			<xsd:pattern value="[0-9]{3}"></xsd:pattern>
                    		</xsd:restriction>
                    	</xsd:simpleType>
                    </xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="Retorno">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>
    				<xsd:element name="tid" type="tns:tidType"
    					maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="pan" type="xsd:string"
    					maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="dados-pedido"
    					type="tns:DadosPedido" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="forma-pagamento"
    					type="tns:FormaPagamento" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="status" type="tns:statusType"
    					maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="autenticacao"
    					type="tns:ProcessamentoAutenticacao" maxOccurs="1"
    					minOccurs="0">
    				</xsd:element>
    				<xsd:element name="autorizacao"
    					type="tns:ProcessamentoAutorizacao" maxOccurs="1"
    					minOccurs="0">
    				</xsd:element>
    				<xsd:element name="captura" type="tns:ProcessamentoCaptura"
    					maxOccurs="1" minOccurs="0">
    				</xsd:element>
    				<xsd:element name="cancelamento"
    					type="tns:Processamento" maxOccurs="1" minOccurs="0">
    				</xsd:element>
    				<xsd:element name="url-autenticacao"
    					type="xsd:string" maxOccurs="1" minOccurs="0">
    				</xsd:element>

    				<xsd:element name="token" type="tns:Token"
    					maxOccurs="1" minOccurs="0">
    				</xsd:element>

    				<xsd:element name="cancelamentos"
    					type="tns:CancelamentoMultiplo" maxOccurs="1" minOccurs="0">
    				</xsd:element>
    				<xsd:element name="dados-portador" type="tns:DadosCartao" maxOccurs="1" minOccurs="0"></xsd:element>
    				<xsd:element name="analise-fraude-retorno" type="tns:AnaliseFraudeRetorno" maxOccurs="1" minOccurs="0"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
        
    <xsd:complexType name="AnaliseFraudeRetorno">
		<xsd:sequence>
			<xsd:element name="statusRevisao" type="tns:StatusRevisao"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="acao" type="tns:AcaoAnaliseFraude"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="dataAnaliseFraude" type="tns:TipoDataHora"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="listaDetalhesAnaliseFraude" type="tns:ListaDetalhesAnaliseFraude"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="scoreAnaliseFraude" type="tns:ScoreAnaliseFraude"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="nivelRiscoAnaliseFraude" type="tns:NivelRiscoAnaliseFraude"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="recomendacaoAnaliseFraude" type="tns:RecomendacaoAnaliseFraude"
				minOccurs="0" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="TipoDataHora">
		<xsd:annotation>
			<xsd:documentation>
				Data hora no formato dd/mm/aaaa hh24:mi:ss. Exemplo 31/12/2008 14:23:00
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{2}(/)\d{2}(/)\d{4}\s\d{2}(:)\d{2}(:)\d{2}"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="StatusRevisao">
		<xsd:annotation>
			<xsd:documentation>
				Status da revisão da análise de fraude (Em
				andamento, Concluida)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="20"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="AcaoAnaliseFraude">
		<xsd:annotation>
			<xsd:documentation>
				Descreve se a Análise de fraude disparou uma ação automática ou manual.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="20"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="ListaDetalhesAnaliseFraude">
		<xsd:annotation>
			<xsd:documentation>Lista dos detalhes da Análise de Fraude
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="detalhesAnaliseFraude" type="tns:DetalhesAnaliseFraude"
				minOccurs="0" maxOccurs="100"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="DetalhesAnaliseFraude">
		<xsd:annotation>
			<xsd:documentation>Resultado legível de cada fase de detalhada
				justificando o resultado da análise de fraude, de modo a instruir o
				lojista a tomar uma ação manual (aceitar ou rejeitar).
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"></xsd:minLength>
			<xsd:maxLength value="255"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ScoreAnaliseFraude">
		<xsd:annotation>
			<xsd:documentation>Valor de 0 a 100 correspondendo ao risco calculado
				para a transação.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:maxInclusive value="100"></xsd:maxInclusive>
			<xsd:minInclusive value="0"></xsd:minInclusive>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NivelRiscoAnaliseFraude">
		<xsd:annotation>
			<xsd:documentation>Informativo do Nivel de risco (alto, médio, baixo)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="50"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="RecomendacaoAnaliseFraude">
		<xsd:annotation>
			<xsd:documentation>Instrução para aceitar ou rejeitar a transação de
				acordo com o nível de risco e os detalhes da análise.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="50"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

    <xsd:simpleType name="tidType">
    	<xsd:restriction base="xsd:string">
    		<xsd:pattern value="[0-9a-zA-Z]{20,40}"></xsd:pattern>

    	</xsd:restriction>
    </xsd:simpleType>

	<xsd:simpleType name="numeroPedidoType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9]{1,20}"></xsd:pattern>
			<xsd:minLength value="1"></xsd:minLength>
			<xsd:maxLength value="20"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="softDescriptorType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[\s*a-zA-Z0-9]{0,13}"></xsd:pattern>
				<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="13"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tokenType">
		<xsd:restriction base="xsd:string">			
			<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="100"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

    <xsd:complexType name="Processamento">
    	<xsd:sequence>
    		<xsd:element name="codigo" type="tns:statusType" maxOccurs="1" minOccurs="1">
    		</xsd:element>
    		<xsd:element name="mensagem" type="xsd:string" maxOccurs="1" minOccurs="1">
    		</xsd:element>
    		<xsd:element name="data-hora" type="xsd:dateTime" maxOccurs="1" minOccurs="1">
    		</xsd:element>
    		<xsd:element name="valor" maxOccurs="1" minOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[0-9]{1,12}"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="datahoraType">
    	<xsd:restriction base="xsd:dateTime">

    	</xsd:restriction>
    </xsd:simpleType>


    <xsd:element name="transacao" type="tns:Retorno"></xsd:element>

    <xsd:complexType name="RequisicaoErro">
    	<xsd:sequence>
    		<xsd:element name="codigo" type="xsd:string"></xsd:element>
    		<xsd:element name="mensagem" type="xsd:string"></xsd:element>
    	</xsd:sequence>
    		<xsd:attribute name="id" type="xsd:string"></xsd:attribute>
    </xsd:complexType>

    <xsd:element name="erro" type="tns:RequisicaoErro"></xsd:element>

    
    <xsd:complexType name="RequisicaoTid">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>
    				<xsd:element name="dados-ec" type="tns:DadosEc" maxOccurs="1" minOccurs="1"></xsd:element>
    				<xsd:element name="forma-pagamento" type="tns:FormaPagamento" maxOccurs="1" minOccurs="1"></xsd:element>
    			</xsd:sequence>

    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="requisicao-tid" type="tns:RequisicaoTid"></xsd:element>

    <xsd:complexType name="RetornoTid">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>
    				<xsd:element name="tid" type="tns:tidType" maxOccurs="1" minOccurs="1"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="RetornoToken">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>


    				<xsd:element name="token" type="tns:Token"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    

    <xsd:simpleType name="idiomaType">
    	<xsd:restriction base="xsd:string">
    		<xsd:enumeration value="PT"></xsd:enumeration>
    		<xsd:enumeration value="EN"></xsd:enumeration>
    		<xsd:enumeration value="ES"></xsd:enumeration>
    		<xsd:length value="2"></xsd:length>
    	</xsd:restriction>
    </xsd:simpleType>

    <xsd:element name="retorno-tid" type="tns:RetornoTid"></xsd:element>
    
    <xsd:element name="retorno-token" type="tns:RetornoToken"></xsd:element>

    <xsd:simpleType name="statusType">
    	<xsd:annotation>
    		<xsd:documentation>
    			1 Em andamento 2 Autenticada 3 Não autenticada 4 Pendente 5 Não autorizada 6
    			Capturada 7 Capturada parcialmente 8 Não capturada 9 Cancelada 10(0) Em autenticação 11(10) Em cancelamento
    		</xsd:documentation>
    	</xsd:annotation>
    	<xsd:restriction base="xsd:string">
    		<xsd:enumeration value="1"></xsd:enumeration>
    		<xsd:enumeration value="2"></xsd:enumeration>
    		<xsd:enumeration value="3"></xsd:enumeration>
    		<xsd:enumeration value="4"></xsd:enumeration>
    		<xsd:enumeration value="5"></xsd:enumeration>
    		<xsd:enumeration value="6"></xsd:enumeration>
    		<xsd:enumeration value="7"></xsd:enumeration>
    		<xsd:enumeration value="8"></xsd:enumeration>
    		<xsd:enumeration value="9"></xsd:enumeration>
    		<xsd:enumeration value="0"></xsd:enumeration>
    		<xsd:enumeration value="10"></xsd:enumeration>
    		<xsd:enumeration value="11"></xsd:enumeration>
    	</xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="BrowserType">
    	<xsd:sequence>
    		<xsd:element name="user-agent" type="xsd:string"></xsd:element>
    		<xsd:element name="accept" type="xsd:string"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ProcessamentoAutenticacao">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Processamento">
    			<xsd:sequence>
    				<xsd:element name="eci" maxOccurs="1"
    					minOccurs="0">

    					<xsd:simpleType>
    						<xsd:restriction base="xsd:int">
    							<xsd:pattern value="[0-9]{0,2}"></xsd:pattern>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ProcessamentoAutorizacao">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Processamento">
    			<xsd:sequence>
    				<xsd:element name="lr" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">

    							<xsd:minLength value="1"></xsd:minLength>
    							<xsd:maxLength value="4"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="arp" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:pattern value="\w{0,6}"></xsd:pattern>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="nsu" minOccurs="0"
    					maxOccurs="1">
    					<xsd:annotation>
    						<xsd:documentation>NSU</xsd:documentation>
    					</xsd:annotation>
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:pattern value="\d{6}"></xsd:pattern>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="codigo-avs-cep" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    				<xsd:element name="mensagem-avs-cep" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    				<xsd:element name="codigo-avs-end" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    				<xsd:element name="mensagem-avs-end" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="requisicao-transacao" type="tns:RequisicaoNovaTransacao"></xsd:element>

    <xsd:complexType name="CancelamentoMultiplo">
    	<xsd:sequence>
    		<xsd:element name="cancelamento" type="tns:Processamento" minOccurs="1" maxOccurs="unbounded"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Token">
    	<xsd:sequence>
    		<xsd:element name="dados-token" type="tns:DadosToken" minOccurs="0" maxOccurs="1"></xsd:element>
    		<xsd:element name="erro" type="tns:RequisicaoErro" minOccurs="0" maxOccurs="1"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="taxaEmbarqueType">
        <xsd:annotation>
        	<xsd:documentation>
        		Valor com formato somente numeros Ex: 1.020,99  deve ser enviado como 102099
			</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
        	<xsd:pattern value="[0-9]{1,9}"></xsd:pattern>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ProcessamentoCaptura">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Processamento">
    			<xsd:sequence>
    				<xsd:element name="valorTaxaEmbarque" maxOccurs="1" minOccurs="0">
		    			<xsd:simpleType>
		    				<xsd:restriction base="xsd:string">
		    					<xsd:pattern value="[0-9]{1,12}"></xsd:pattern>
		    				</xsd:restriction>
		    			</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    	<xsd:complexType name="RequisicaoNovaTransacaoCelular">
    	<xsd:complexContent>
    		<xsd:extension base="tns:Mensagem">
    			<xsd:sequence>
    				<xsd:element name="dados-ec"
    					type="tns:DadosEcAutenticacao" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="dados-portador"
    					type="tns:DadosCelular" maxOccurs="1" minOccurs="0">
    				</xsd:element>
    				<xsd:element name="dados-pedido"
    					type="tns:DadosPedido" maxOccurs="1" minOccurs="1">
    				</xsd:element>

    				<xsd:element name="forma-pagamento"
    					type="tns:FormaPagamentoCelular" maxOccurs="1" minOccurs="1">
    				</xsd:element>
    				<xsd:element name="url-retorno" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">    			
    						    <xsd:minLength value="0"></xsd:minLength>				
    							<xsd:maxLength value="1024"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="capturar" type="xsd:boolean"
    					maxOccurs="1" minOccurs="1">
                        <xsd:annotation>
                        	<xsd:documentation>
                        		Define se a transacao sera capturada apos uma autorizacao com
                        		sucesso.
                        	</xsd:documentation>
                        </xsd:annotation>
    				</xsd:element>
    				<xsd:element name="campo-livre" maxOccurs="1"
    					minOccurs="0">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:maxLength value="128"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
					<xsd:element name="gerar-token" type="xsd:boolean"
    					maxOccurs="1" minOccurs="0">
    					<xsd:annotation>
    						<xsd:documentation>
    							Define se a transacao deverá gerar um
    							token para o "Cartão + EC".
    						</xsd:documentation>
    					</xsd:annotation>
    				</xsd:element>
    				<xsd:element name="avs" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
	    <xsd:complexType name="DadosCelular">
    	<xsd:sequence>
    		<xsd:element name="ddd" minOccurs="1" maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:length value="2"></xsd:length>
    					<xsd:pattern value="[0-9]{2}"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="numero" maxOccurs="1" minOccurs="1">
                <xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="[0-9]{8,9}"/>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
       	</xsd:sequence>
    </xsd:complexType>
    
 <xsd:complexType name="FormaPagamentoCelular">
    	<xsd:sequence>
            <xsd:element name="produto" maxOccurs="1" minOccurs="1">
    			<xsd:annotation>
    				<xsd:documentation>
    					1 - Crédito a Vista 2 - Parcelado Loja 3 - Parcelado Adm A - Visa Electron
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:pattern value="(1|2|3|A)"></xsd:pattern>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="parcelas" maxOccurs="1" minOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:int">
    					<xsd:minInclusive value="1"></xsd:minInclusive>
    					<xsd:maxInclusive value="999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
	
	<xsd:element name="requisicao-nova-transacao-celular" type="tns:RequisicaoNovaTransacaoCelular"></xsd:element>
        
    <xsd:complexType name="DadosAutenticacaoMPIExterna">
        <xsd:sequence>
            <xsd:element name="cavv" maxOccurs="1" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"></xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="xid" maxOccurs="1" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"></xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
<!-- Campos que configuram o comportamento da analise de fraude -->
    <xsd:complexType name="ConfiguracaoAnaliseFraude">
        <xsd:sequence>
            <xsd:element name="analisar-fraude" type="xsd:boolean" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
					<xsd:documentation>
                            Define se a transacao sera passada pela analise de fraude. 
					</xsd:documentation>
				</xsd:annotation>
            </xsd:element>
			<xsd:element name="alto-risco" type="xsd:string" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
					<xsd:documentation>
                            Define o que será feito com a transação caso retorne que seja de Alto Risco da Analise de Fraude
					</xsd:documentation>
				</xsd:annotation>
            </xsd:element>
			<xsd:element name="medio-risco" type="xsd:string" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
					<xsd:documentation>
                            Define o que será feito com a transação caso retorne que seja de Medio Risco da Analise de Fraude
					</xsd:documentation>
				</xsd:annotation>
            </xsd:element>
			<xsd:element name="baixo-risco" type="xsd:string" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
					<xsd:documentation>
                            Define o que será feito com a transação caso retorne que seja de Baixo Risco da Analise de Fraude
					</xsd:documentation>
				</xsd:annotation>
            </xsd:element>
			<xsd:element name="erro-dados" type="xsd:string" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
					<xsd:documentation>
                            Define o que será feito com a transação caso retorne um erro de dados da Analise de Fraude
					</xsd:documentation>
				</xsd:annotation>
            </xsd:element>
			<xsd:element name="erro-indisponibilidade" type="xsd:string" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
					<xsd:documentation>
                            Define o que será feito com a transação caso retorne um erro de indisponibilidade da Analise de Fraude
					</xsd:documentation>
				</xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AnaliseFraude">
        <xsd:sequence>
            
            <xsd:element name="configuracao" type="tns:ConfiguracaoAnaliseFraude" maxOccurs="1" minOccurs="1"></xsd:element>
            
            <xsd:element name="afsService_run" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Requisição do serviço de Advanced Fraud Screen.
                        Valores válidos: true/false
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="true"></xsd:enumeration>
                        <xsd:enumeration value="false"></xsd:enumeration>
                        <xsd:maxLength value="5"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="merchantReferenceCode" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Número do pedido ou de referência; deve ser único.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="50"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="billTo_street1" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Endereço de cobrança.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="billTo_street2" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Complemento do endereço de cobrança.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="billTo_city" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Cidade do endereço de cobrança.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="50"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="billTo_state" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Estado do endereço de cobrança.
                        Obrigatório para EUA e Canadá.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="2"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="billTo_country" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        País do endereço de cobrança.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="2"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="billTo_postalCode" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        CEP do endereço de cobrança.
                        Obrigatório para EUA e Canadá.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="10"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="billTo_customerID" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        ID do cliente.
                        Pode ser número do cartão fidelidade.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="50"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element name="billTo_customerPassword" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Senha da conta de acesso do cliente.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="255"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="billTo_personalID" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Dados de CPF do cliente.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="26"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="billTo_email" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        E-mail do cliente.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="100"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                        
                                        
            <xsd:element name="billTo_firstName" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Nome do cliente.
                        Deve ser o mesmo do titular do cartão de crédito.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element> 
            
            <xsd:element name="billTo_lastName" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Sobrenome do cliente.
                        Deve ser o mesmo do titular do cartão de crédito.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="billTo_phoneNumber" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Telefone do cliente do endereço de cobrança.
                        Apenas números.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">                        
                        <xsd:pattern value="[0-9]{0,15}"></xsd:pattern>                        
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                     
                                    
            <xsd:element name="billTo_ipAddress" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Endereço IP do cliente.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="15"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                 
                                                                        
            <xsd:element name="shipto_street1" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Endereço de envio.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>              
            
            <xsd:element name="shipto_street2" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Complemento do endereço de envio.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>     
            
            <xsd:element name="shipto_city" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Cidade do endereço de envio.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="50"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                         
                                
            <xsd:element name="shipto_state" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Estado do endereço de envio.
                        Obrigatório para EUA e Canadá.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:length value="2"></xsd:length>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element> 
            
            <xsd:element name="shipto_country" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        País do endereço de envio.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:length value="2"></xsd:length>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element> 
            
            <xsd:element name="shipto_postalCode" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        CEP do endereço de envio.
                        Obrigatório para EUA e Canadá.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="10"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                                                                                                                                 
                                                                                                                                          
            <xsd:element name="shipTo_phoneNumber" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Telefone do cliente do endereço de envio.
                        Apenas números.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[0-9]{0,15}"></xsd:pattern>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                                                                                                                                                                                                 
                                                    
            <xsd:element name="deviceFingerprintID" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        ID da impressão digital.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="88"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                                                                                                                                                                                                    
                                                                                                                                                                      
            <xsd:element name="decisionManager_travelData_completeRoute" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Concatenação das pernas dos vôos.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="255"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                                                                                                                                                                                                                   
                                                                                               
            <xsd:element name="decisionManager_travelData_departureDateTime" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Data e hora da partida do vôo.
                        Formatos válidos:
                            yyyy-MM-dd HH:mm z
                            yyyy-MM-dd hh:mm a z
                            yyyy-MM-dd hh:mma z
                        Legendas:
                            HH = hora no formato 24 horas
                            hh = hora no formato 12 horas
                            a = am ou pm (case insensitive)
                            z = timezone
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="25"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                   
                                                                            
            <xsd:element name="decisionManager_travelData_journeyType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Tipo de viagem (one way ou round trip)
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="32"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element> 

            <xsd:element name="decisionManager_travelData_leg_origin" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Código do aeroporto de partida
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="3"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>             
                                                                                                                                                                                                           
            <xsd:element name="decisionManager_travelData_leg_destination" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Código do aeroporto de chegada.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="3"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>  

            <xsd:element name="purchaseTotals_currency" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Moeda.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="5"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                        

            <xsd:element name="purchaseTotals_grandTotalAmount" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Valor total do pedido.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:decimal">
                        <xsd:totalDigits value="15"></xsd:totalDigits>
                        <xsd:fractionDigits value="2"></xsd:fractionDigits>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                            

            <xsd:element name="item_unitPrice" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Preço do produto, por item.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:element>  

            <xsd:element name="item_passengerFirstName" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Nome do passageiro.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>               
            
            <xsd:element name="item_passengerLastName" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Sobrenome do passageiro.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="60"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                    

            <xsd:element name="item_passengerEmail" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        E-mail do passageiro.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="255"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>  
            
            <xsd:element name="item_passengerID" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        ID do passageiro.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="32"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element> 
            
            <xsd:element name="item_passengerPhone" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Telefone de contato do passageiro.
                        Apenas números, é recomendável o 
                        preenchimento do código do país.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[0-9]{0,15}"></xsd:pattern>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element> 
                                 
            <xsd:element name="item_passengerStatus" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Classificação do passageiro de acordo com companhia aérea.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="32"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             

            <xsd:element name="item_passengerType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Classificação do passageiro de acordo com preço do ticket.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="32"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>  

            <xsd:element name="item_quantity" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Quantidade dos produtos sendo adquiridos.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[0-9]{0,10}"></xsd:pattern>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="item_productCode" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Tipo de produto.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="255"></xsd:maxLength>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>  
            
            <xsd:element name="installment_totalCount" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Número de parcelas.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[0-9]{0,2}"></xsd:pattern>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
            <xsd:element name="mdd"
                    type="tns:MDD" maxOccurs="1" minOccurs="0">
            </xsd:element>
            
        </xsd:sequence>
    </xsd:complexType>        
	
    <xsd:complexType name="MDD">
    	<xsd:sequence>
            <xsd:element name="merchantDefinedData_mddField_6" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="255"></xsd:maxLength>
    					</xsd:restriction>
				</xsd:simpleType>
            </xsd:element>
            <xsd:element name="merchantDefinedData_mddField_7" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="255"></xsd:maxLength>
    					</xsd:restriction>
				</xsd:simpleType>
            </xsd:element>
            <xsd:element name="merchantDefinedData_mddField_8" maxOccurs="1" minOccurs="0">
                        <xsd:annotation>
    				<xsd:documentation>
    					Informar se o número do cartão foi digitado ou colado.
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="255"></xsd:maxLength>
    					</xsd:restriction>
				</xsd:simpleType>
            </xsd:element>
            <xsd:element name="merchantDefinedData_mddField_9" maxOccurs="1" minOccurs="0">
                        <xsd:annotation>
    				<xsd:documentation>
    					Informar se o endereço de E-mail foi digitado ou colado.
    				</xsd:documentation>
    			</xsd:annotation>
    			<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="255"></xsd:maxLength>
    					</xsd:restriction>
				</xsd:simpleType>
            </xsd:element>
            <xsd:element name="merchantDefinedData_mddField_10" maxOccurs="1" minOccurs="0">
    			<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="255"></xsd:maxLength>
    					</xsd:restriction>
				</xsd:simpleType>
